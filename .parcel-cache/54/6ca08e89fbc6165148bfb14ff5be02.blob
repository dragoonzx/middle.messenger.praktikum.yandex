.settings {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  height: 100%;
}
.settings__image {
  width: 132px;
  height: 132px;
}
.settings__name {
  font-weight: 600;
  font-size: 16px;
  margin-top: 16px;
}
.settings__fields {
  width: 510px;
}
.settings__list {
  list-style-type: none;
  margin: 0;
  padding: 0;
}
.settings__list-item {
  display: flex;
  justify-content: space-between;
  flex-basis: 510px;
}