n  submitButton.addEventListener('click', () => {\n    logFormData()\n  })\n\n})()\n"],"names":["document","addEventListener","querySelector","e","avatarNode","target","files","textContent","name","style","fieldsValues","querySelectorAll","forEach","field","formKey","dataset","fieldNode","console","log","fieldValue","logFormData"],"version":3,"file":"settings-avatar.HASH_REF_44c96c533efcf12db70e6f993f4b8353.js.map"}